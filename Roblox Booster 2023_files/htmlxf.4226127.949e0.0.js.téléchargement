        //Old locker
        //Specific CSS
        xfContentLocker.setTemplateCSSDir(cssDIR);
        xfContentLocker.removeElByID("xf_MODAL_CONTAINER");


        //HTML
        xfContentLocker.removeElByID("xf_MODAL_CONTAINER");
